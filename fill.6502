CMP #$28  ;
BNE #$F1  ; go back to plotting $35C
; increment and check X
INC $FA   ;
LDA $FA   ;
CMP #$28  ;
BNE #$E5  ; go back to inner loop init $358
BEQ #$03  ; end this
JMP $035C ; goto plotting $35C
RTS       ; done