$350      ;
; init 
LDA #$11  ; set color to red
STA $30   ;
; outer loop init 
LDA #$00  ; X = 0
STA $FA   ;
; inner loop init
LDA #$00  ; Y = 0
STA $FB   ;
; plotting
LDA $FA   ;
